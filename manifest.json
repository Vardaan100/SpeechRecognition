{
    "manifest_version": 2,
  
    "name": "Voice assistant",
    "description": "Speech assistant",
    "version": "1.1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "background" : {
        "page" : "popup.html"
    },
    
    "web_accessible_resources": [
          "permissions.html"
        ],
    
    "permissions": [
     "tabs",
     "storage"
    ],
    "chrome_url_overrides":{
        "newtab":"popup.html"
    }
}   